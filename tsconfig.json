{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "lib": ["es2022", "dom", "dom.iterable"],
    "skipLibCheck": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "strictBindCallApply": false,
    "plugins": [
      { "name": "@astrojs/ts-plugin" }
    ]
  }
}
